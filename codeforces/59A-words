#include <iostream>
#include<string>
 
using namespace std;
 
int main()
{
   int i,up=0,lo=0;
   
   string s;
   
   cin>>s;
   
   for(i=0 ; i<s.size() ; i++)
   {
       if(isupper(s[i]))
       {
           up++;
       }
       else
       {
           lo++;
       }
   }
   
   if(up<lo)
   {
       for(i=0 ; i<s.size() ; i++)
       {
           s[i]=tolower(s[i]);
       }
       
       cout<<s<<endl;
   }
   else if(up>lo)
  {
      for(i=0 ; i<s.size() ; i++)
       {
           s[i]=toupper(s[i]);
       }
       
       cout<<s<<endl;
  }
  
   else if(up==lo)
  {
      for(i=0 ; i<s.size() ; i++)
       {
           s[i]=tolower(s[i]);
       }
       
       cout<<s<<endl;
  }
 
    return 0;
}
